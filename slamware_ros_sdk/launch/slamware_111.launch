<launch>
  <arg name="ip_address" default="192.168.11.3" />

  <node name="wowowoo02" pkg="slamware_ros_sdk" type="slamware_ros_sdk_server_node" output="screen">

  <param name = "ip_address"                value = "$(arg ip_address)"/>
  <param name = "angle_compensate"         value = "false"/>
  <param name = "fixed_odom_map_tf"         value = "false"/>

  <param name = "robot_frame"              value = "/base_link02"/>
  <param name = "odom_frame"               value = "/odom02"/>
  <param name = "laser_frame"              value = "/laser02"/>
  <param name = "map_frame"                value = "/slamware_map02"/>

  <param name = "robot_pose_pub_period"    value = "0.05"/>
  <param name = "scan_pub_period"          value = "0.1"/>
  <param name = "map_pub_period"           value = "0.2"/>
  <param name = "path_pub_period"          value = "0.05"/>

  <param name = "/cmd_vel"                 value = "/cmd_vel02"/>
  <param name = "/move_base_simple/goal"   value = "/move_base_simple/goal"/>
  <param name = "scan"                     value = "scan02"/>
  <param name = "odom"                     value = "odom"/>
  <param name = "map"                      value = "map"/>
  <param name = "map_metadata"             value = "map_metadata02"/>
  <param name = "global_plan_path"         value = "global_plan_path02"/>

  <!-- topic remap /-->
  <remap from = "/cmd_vel"                   to = "/cmd_vel02"/>
  <remap from = "/move_base_simple/goal"     to = "/move_base_simple/goal02"/>
  <remap from = "/scan"                       to = "scan02"/>
  <remap from = "/odom"                       to = "odom02"/>
  <remap from = "/tf"                       to = "tf02"/>

  <remap from = "map"                        to = "slamware_map02"/>
  <remap from = "map_metadata"               to = "map_metadata02"/>
  <remap from = "global_plan_path"           to = "global_plan_path02"/>

  

  </node>

  <node name="base2laser2" pkg="tf" type="static_transform_publisher" args="0.35 0 0 0 0 0 1 /base_link /laser02 100"/>



</launch>
